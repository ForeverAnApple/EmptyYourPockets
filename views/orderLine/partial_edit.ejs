<div class=row>
    <div class="col col-xs-12">
      <div class="form-group">
        <label for="orderID">orderID</label>
        <input type="number" class="form-control" id="orderID" name="orderID" value="<%= orderLine.orderID %>"  required>
      </div>
      <div class="form-group">
        <label for="lineNumber">lineNumber</label>
        <input type="number" class="form-control" id="lineNumber" name="lineNumber" value="<%= orderLine.lineNumber %>"  required>
      </div>
      <div class="form-group">
        <label for="productID">product ID</label>
        <input type="text" class="form-control" id="productID" name="productID" value="<%= orderLine.productID %>"  required>
      </div>
      <div class="form-group">
        <label for="quantity">Total amount</label>
        <input type="number" class="form-control" id="quantity" name="quantity" value="<%= orderLine.quantity %>" required>
      </div>
    </div>
  </div>
